<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Supplier_name__c</field>
        <field>Buyer_Name__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>mel.dinh@incomlend.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>trustee_approval</label>
        <name>trustee_approval</name>
    </approvalStep>
    <emailTemplate>Incomlend_emails/Onboarding_Approval_Map_Camelot</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5</booleanFilter>
        <criteriaItems>
            <field>Supplier_Buyer_Map__c.RPA_Signed__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Supplier_Buyer_Map__c.BNL_Signed__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Supplier_Buyer_Map__c.Trustee__c</field>
            <operation>equals</operation>
            <value>Amicorp Trust</value>
        </criteriaItems>
        <criteriaItems>
            <field>Supplier_Buyer_Map__c.supplier_onboarding_stage__c</field>
            <operation>equals</operation>
            <value>99.Onboarded</value>
        </criteriaItems>
        <criteriaItems>
            <field>Supplier_Buyer_Map__c.buyer_onboarding_stage__c</field>
            <operation>equals</operation>
            <value>99.Onboarded</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>Map: onboarding approval trustee Camelot</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
